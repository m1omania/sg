{
  "name": "solar-group-server",
  "version": "1.0.0",
  "description": "SolarGroup backend server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "postinstall": "npm rebuild sqlite3",
    "migrate": "node scripts/migrate.js",
    "migrate:up": "node scripts/migrate.js up",
    "migrate:status": "node scripts/migrate.js status",
    "migrate:validate": "node scripts/migrate.js validate",
    "backup": "node scripts/backup.js",
    "backup:create": "node scripts/backup.js create",
    "backup:list": "node scripts/backup.js list",
    "backup:restore": "node scripts/backup.js restore",
    "backup:cleanup": "node scripts/backup.js cleanup",
    "backup:export": "node scripts/backup.js export",
    "backup:check": "node scripts/backup.js check",
    "backup:optimize": "node scripts/backup.js optimize",
    "db:monitor": "node scripts/db-monitor.js",
    "db:stats": "node scripts/db-monitor.js stats",
    "db:recommendations": "node scripts/db-monitor.js recommendations",
    "db:cache": "node scripts/db-monitor.js cache",
    "check": "node ../scripts/check-compatibility.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.1",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "express-slow-down": "^2.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "babel-jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-watch-typeahead": "^2.2.2"
  },
  "keywords": ["solar", "group", "investment", "coupons"],
  "author": "SolarGroup Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  }
}