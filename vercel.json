{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "../app.js",
          "../deposit.html",
          "../index.html",
          "../invest.html",
          "../landing.html",
          "../my-investments.html",
          "../projects.html",
          "../register.html",
          "../style.css",
          "../wallet.html"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/server.js"
    }
  ]
}